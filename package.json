{
  "name": "labx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-ide/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:ui": "vite",
    "dev:ide": "npm run transpile:ide && electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:ide": "tsc --project src/ide/tsconfig.json",
    "dist:mac": "npm run transpile:ide && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:ide && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run transpile:ide && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tailwindcss/vite": "^4.1.10",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "monaco-editor": "^0.52.2",
    "multer": "^2.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-rnd": "^10.5.2",
    "react-router": "^7.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
